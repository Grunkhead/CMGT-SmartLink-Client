{
  "author": "Dave Buitenhuis",
  "name": "CMGT SmartLink",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "version": "1.1.1",
  "description": "",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
